<% include header %>

<h2>새로운 식단 요청</h2>

<form method="post" role="form">
  <div class="form-group">
    <label for="title">제목</label>
    <input type="text" class="form-control" id="title" name="title" placeholder="제목을 입력하세요" required>
  </div>
  <div class="form-group">
    <label for="body">요청 내용</label>
    <textarea class="form-control" id="body" name="body" placeholder="원하시는 메뉴룰 적어주세요" rows="3" required></textarea>
  </div>
  <div class="form-group">
    <input type="submit" value="식단 요청 등록" class="btn btn-primary">
  </div>
</form>

<% include footer %>
